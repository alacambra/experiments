<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.main {
			margin: auto;
		}
	</ui:style>

	<g:HTMLPanel>
		<g:VerticalPanel ui:field="mainPanel" styleName="{style.main}">
			<g:HorizontalPanel ui:field="naviPanel">
				<g:Button ui:field="newInvoiceBtn" text="New Invoice"></g:Button>
				<g:Button ui:field="newBudgetBtn" text="New Budget"></g:Button>
			</g:HorizontalPanel>
			<g:HTMLPanel ui:field="contentPanel"></g:HTMLPanel>
		</g:VerticalPanel>

	</g:HTMLPanel>
</ui:UiBinder>
