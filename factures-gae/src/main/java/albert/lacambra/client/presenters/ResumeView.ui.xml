<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.containersFrame {
			margin: 7px;
		}
		
		.mainFrame {
			border: solid 1px black;
		}
		
	</ui:style>

	<g:HTMLPanel styleName="{style.mainFrame}">
		<g:HorizontalPanel>
			<g:VerticalPanel styleName="{style.containersFrame}" ui:field="budgetsNamePanel">
			</g:VerticalPanel>
			<g:VerticalPanel styleName="{style.containersFrame}" ui:field="quantitiesPanel">
			</g:VerticalPanel>
		</g:HorizontalPanel>
		<g:Button ui:field="refreshBtn" text="load"></g:Button>
	</g:HTMLPanel>

</ui:UiBinder>
